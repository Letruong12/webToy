<style>
    .error-box .error-body .error-title {
        font-size: 210px !important;
        font-weight: 900 !important;
        line-height: 210px !important
    }
    .error-box {
        height: 100%;
        width: 100%;
        position: fixed
    }
    .error-box .error-body {
        padding-top: 5%;
        height: 100%;
        width: 100%;
        position: fixed
    }
    .error-box .error-body p {
        font-size: 16px;
        font-weight: 300;
        line-height: 24px
    }
    .error-box .error-body .btn-info {
        background: #1e88e5;
        border: 1px solid #1e88e5
    }
