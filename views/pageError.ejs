    }
    .error-box .error-title {
        font-size: 210px;
        font-weight: 900;
        color: #455a64;
        line-height: 210px
    }
    .error-box .error-subtitle {
        font-size: 21px;
        line-height: 30px;
        font-weight: 400
    }
    .error-box .footer {
